<main id="main"> 
 
  <!-- ======= Breadcrumbs ======= -->
  <%= render 'layouts/topbar' %>
  <!-- End Breadcrumbs -->
 
  <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="container">

        <div class="d-flex justify-content-between align-items-center">
          <h2><%= t('layouts.header.services') %></h2></h2>
          <ol>
            <li><a href="/">Home</a></li>
            <li><%= t('layouts.header.services') %></li>
          </ol>
        </div>

      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Services Section ======= -->
    <%= render 'layouts/ourservices' %>
    <!-- End Services Section -->

    <!-- ======= Features Section ======= -->
    <section id="features" class="features">
      <div class="container">

        <div class="section-title" data-aos="fade-up">
          <h2>
          <%= t('layouts.services.some') %>
          <strong><%= t('layouts.services.features') %></strong> 
          <%= t('layouts.services.we_provide') %></h2>
          <p><%= t('layouts.services.needs') %></p>
        </div>

        <div class="row">
          <div class="col-lg-4 mb-5 mb-lg-0" data-aos="fade-right">
            <ul class="nav nav-tabs flex-column">
              <li class="nav-item">
                <a class="nav-link active show" data-toggle="tab" href="#tab-1">
                  <h4><%= t('layouts.services.active_listening') %></h4>
                  <p><%= t('layouts.services.active_listening_meaning') %></p>
                </a>
              </li>
              <li class="nav-item mt-2">
                <a class="nav-link" data-toggle="tab" href="#tab-2">
                  <h4><%= t('layouts.services.hands-on_experience') %></h4>
                  <p><%= t('layouts.services.hands-on_experience_meaning') %></p>
                </a>
              </li>
              <li class="nav-item mt-2">
                <a class="nav-link" data-toggle="tab" href="#tab-3">
                  <h4><%= t('layouts.services.deliver_the_value') %></h4>
                  <p><%= t('layouts.services.deliver_the_value_meaning') %></p>
                </a>
              </li>
              <li class="nav-item mt-2">
                <a class="nav-link" data-toggle="tab" href="#tab-4">
                  <h4>Feedback</h4>
                  <p><%= t('layouts.services.feedback_meaning') %></p>
                </a>
              </li>
            </ul>
          </div>
          <div class="col-lg-7 ml-auto" data-aos="fade-left" data-aos-delay="100">
            <div class="tab-content">
              <div class="tab-pane active show" id="tab-1">
                <figure>
                  <img src="staticos/assets/img/features-1.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-2">
                <figure>
                  <img src="staticos/assets/img/features-2.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-3">
                <figure>
                  <img src="staticos/assets/img/features-3.png" alt="" class="img-fluid">
                </figure>
              </div>
              <div class="tab-pane" id="tab-4">
                <figure>
                  <img src="staticos/assets/img/features-4.png" alt="" class="img-fluid">
                </figure>
              </div>
            </div>
          </div>
        </div>

      </div>
    </section><!-- End Features Section -->

  </main><!-- End #main -->

  <!-- ======= Footer ======= -->
  <%= render 'layouts/footer' %>
  <!-- End Footer -->

  <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>

  <!-- Vendor JS Files -->
  <script src="staticos/assets/vendor/jquery/jquery.min.js"></script>
  <script src="staticos/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="staticos/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
  <script src="staticos/assets/vendor/php-email-form/validate.js"></script>
  <script src="staticos/assets/vendor/jquery-sticky/jquery.sticky.js"></script>
  <script src="staticos/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="staticos/assets/vendor/venobox/venobox.min.js"></script>
  <script src="staticos/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
  <script src="staticos/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
  <script src="staticos/assets/vendor/aos/aos.js"></script>

  <!-- Template Main JS File -->
  <script src="staticos/assets/js/main.js"></script>

</main>